isbn = {978-981-10-7431-8; 978-981-10-7430-1},
issn = {2522-8366},
keywords = {Bioactive compounds,Biopesticides,Biopolymers,Environmental applications,Enzymes,Microbial transformation,NA CP,Organic acids,Review},
mendeley-tags = {Bioactive compounds,Biopesticides,Biopolymers,Environmental applications,Enzymes,Microbial transformation,NA CP,Organic acids,Review},
organization = {Int Soc Energy {\&} Environm Sustainabil; Ctr Innovat {\&} Appl Bioprocessing; Govt India, Dept Sci {\&} Technol; TSI; Tesscorn; AVL; Horiba; Springer},
pages = {97--109},
