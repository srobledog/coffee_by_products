journal = {World Applied Sciences Journal},
keywords = {Enzymes,Microbial transformation},
mendeley-tags = {Enzymes,Microbial transformation},
number = {6},
pages = {781--789},
title = {{Bioconversion of coffee husk cellulose and statistical optimization of process for production of exoglucanase by rhizopus stolonifer}},
url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84871862369{\&}doi=10.5829{\%}2Fidosi.wasj.2012.20.06.6689{\&}partnerID=40{\&}md5=ae9e6a503276925223a4729e1b15dab4},
