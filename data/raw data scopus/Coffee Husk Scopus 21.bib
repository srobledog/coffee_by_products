@ARTICLE{PalominoGarcía2015777,
author={Palomino García, L.R. and Del Bianchi, V.L.},
title={THE EFFECT OF FUNGAL FERMENTATION IN PHENOLICS CONTENT IN ROBUSTA COFFEE HUSK EFEITO DA FERMENTACCAO FUNGICA NO TEOR DE COMPOSTOS FENOLICOS EM CASCA DE CAFE ROBUSTA},
journal={Semina:Ciencias Agrarias},
year={2015},
volume={36},
number={2},
pages={777-786},
doi={10.5433/1679-0359.2015v36n2p777},
note={cited By 3},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84929300439&doi=10.5433%2f1679-0359.2015v36n2p777&partnerID=40&md5=8f8a83b98a7a3845c9474ddab8e5200a},
affiliation={Universidade Estadual Paulista, Instituto de Biociências, Letras e Ciências Exatas, UNESPSP, Brazil; Dept de Engenharia e Tecnologia de Alimentos, UNESP, Campus de São José do Rio PretoSP, Brazil},
abstract={Coffee husk is an abundant by-product generated by the coffee industry and it can be used for the production of-value-added phenolic compounds. Currently, this residue has no commercial use due to the presence of anti-nutritional compounds and it is returned to the soil or burned. The aim of this study was to evaluate the content of phenolic compounds in Robusta coffee husk, the adequacy of this residue as substrate for fermentation processes, as well as evaluating the influence of fungal solid state fermentation to obtain phenolic compounds from this residue. In the present study, the use of different solvents for the extraction of polyphenols was evaluated and the content was found to be in the range of 96.9-159.5 mg of galic acid (GA)·g-1 substrate, depending on the solvent used. The best solvent was acetone, therefore it was selected for extraction. Studies were carried out to evaluate the effect of solid-state fermentation in the release of phenolic compounds, using the filamentous fungi Penicillium purpurogenum. The total phenolic content increased from 159.5 up to 243.2 mg GA·g-1 substrate as a result the solid-state fermentation.},
author_keywords={Bioactive compounds;  Coffea canephora;  Penicillium purpurogenum;  Solid state fermentation},
