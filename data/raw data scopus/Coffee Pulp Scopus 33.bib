@ARTICLE{Pérez-Morales2011352,
author={Pérez-Morales, G.G. and Ramírez-Coronel, A. and Guzmán-López, O. and Cruz-Sosa, F. and Perraud-Gaime, I. and Roussos, S. and Saucedo-Castañeda, G.},
