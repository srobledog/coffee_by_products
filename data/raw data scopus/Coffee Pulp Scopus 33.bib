@ARTICLE{Singh2020686,
author={Singh, A.K. and Sharma, R.},
title={A review on sustainable management of coffee industry by-products},
journal={Journal of Critical Reviews},
year={2020},
volume={7},
number={10},
pages={686-691},
doi={10.31838/jcr.07.10.137},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85088317920&doi=10.31838%2fjcr.07.10.137&partnerID=40&md5=80ad46712246adaf241466fd0dc22e7f},
affiliation={Dept. of Biotechnology, Sharda University, Greater Noida, Uttar Pradesh, India},
abstract={Coffee is one of the world's most popular beverages and the second largest traded product after petroleum. Coffee is produced around the globe in about 80 countries and interferes with huge business around the world. Coffee dispensation needs a high degree of processing know-how and produces large quantities of processing by-products such as coffee pulp and husk that have limited applications such as animal feed, fertilizer, compost, and so on. Biotechnological developments in industrial waste management are supporting the sustainable development of the economy of the country. The aims of food processing by-products, waste and effluents include recovering fine chemicals and developing useful metabolites through chemical and biotechnological processes. Pretreatments, accompanied by rehabilitation procedures, include value-added materials (natural antioxidants, enzymes, vitamins, cellulose, proteins, starch, lipids, and pigments) of great importance for the cosmetic, pharmaceutical, and food industries. With the backdrop of high crop production in coming years, it is imperative to counterpart this development with some use and industrial use of coffee byproducts as the coffee industry produces huge amounts of coffee by-products that are flourishing nutrient resources. The present analysis highlights value-added explorations of coffee by-products that can be accomplished by valuation strategy, incorporation of bioengineering principles techniques and applications in food processing and waste management and, secondly, conservation of environment with disposal issue improving both ecological and economic resources. © 2020 Innovare Academics Sciences Pvt. Ltd. All rights reserved.},
author_keywords={Coffee;  Coffee by-products;  Coffee pulp;  Processing;  Sustainable development;  Waste management},
