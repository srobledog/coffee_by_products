@ARTICLE{Nzekoue2020,
author={Nzekoue, F.K. and Angeloni, S. and Navarini, L. and Angeloni, C. and Freschi, M. and Hrelia, S. and Vitali, L.A. and Sagratini, G. and Vittori, S. and Caprioli, G.},
title={Coffee silverskin extracts: Quantification of 30 bioactive compounds by a new HPLC-MS/MS method and evaluation of their antioxidant and antibacterial activities},
