@ARTICLE{Guglielmetti2019157,
author={Guglielmetti, A. and Fernandez-Gomez, B. and Zeppa, G. and Del Castillo, M.D.},
title={Nutritional quality, potential health promoting properties and sensory perception of an improved gluten-free bread formulation containing inulin, rice protein and bioactive compounds extracted from coffee byproducts},
journal={Polish Journal of Food and Nutrition Sciences},
year={2019},
volume={69},
number={2},
pages={157-166},
doi={10.31883/pjfns-2019-0012},
note={cited By 5},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85063235035&doi=10.31883%2fpjfns-2019-0012&partnerID=40&md5=015f17e242cbc6d10b82254bca4a8cc0},
affiliation={Department of Agricultural, Forest and Food Sciences (DiSAFA), University of Turin, Grugliasco, Torino, 10095, Italy; Department of Food Analysis and Bioactivity, Institute of Food Science Research (CIAL, CSIC-UAM), Campus de la Universidad Autónoma de Madrid, C/ Nicolás Cabrera 9, Madrid, 28049, Spain},
abstract={The present study aimed to improve the formulation of a gluten-free commercial bread-making premix by adding inulin, rice protein and extracts of coffee by-products (silverskin and husk) to obtain healthier products enriched in protein and dietary fiber with the potential to reduce the risk of chronic diseases. The new formulation combines well-known and novel food ingredients. To validate the novel ingredients, we determined the physicochemical characterization, caffeine and chlorogenic acid contents, and food safety of coffee silverskin (CSE) and husk (CHE) extracts. Sensory and nutritional quality, bioactive compounds (chlorogenic acid, total phenolic compounds, melanoidins and browning), in vitro bioaccessibility, and health-promoting properties (overall antioxidant capacity and α-glucosidase activity) of the breads were evaluated as well. Results support the feasibility of CSE and CHE as natural sustainable sources of antioxidants, α-glucosidase inhibitors and colorants. The observed health-promoting properties suggest that coffee by-product extracts could potentially be used as functional food ingredients or supplements to reduce the risk of chronic diseases associated with oxidative stress and to control postprandial glucose levels. Based on consumers' preferences, we obtained new bread formulations with a high nutritional and sensorial quality, suitable for celiacs and with the potential to reduce the risk of gastrointestinal disease related to oxidative stress. Data on in vitro digestion indicated a significant (p<0.05) decrease in the bioaccessibility of sugars and a significant increase (p<0.05) in antioxidants. © Copyright by Institute of Animal Reproduction and Food Research of the Polish Academy of Sciences},
author_keywords={Antioxidant coffee fibre;  Coffee by-products;  Gluten-free bread;  Inulin;  Sugars bioaccesibility;  α-glucosidase},
funding_text 1={The present research was funded by the project SUS-COFFEE (AGL2014–57239-R) and is part of the PhD. thesis of Guglielmetti, A.},
