@ARTICLE{Torres-Valenzuela2020,
author={Torres-Valenzuela, L.S. and Ballesteros-Gómez, A. and Rubio, S.},
title={Supramolecular solvent extraction of bioactives from coffee cherry pulp},
journal={Journal of Food Engineering},
year={2020},
volume={278},
doi={10.1016/j.jfoodeng.2020.109933},
art_number={109933},
note={cited By 6},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85078159804&doi=10.1016%2fj.jfoodeng.2020.109933&partnerID=40&md5=6559992afafce188cc58a8a55e652fa6},
affiliation={Department of Analytical Chemistry, Institute of Fine Chemistry and Nanochemistry, Universidad de Córdoba, Cordoba, 14071, Spain; Faculty of Engineering, Universidad La Gran Colombia, Armenia, 630004, Colombia},
abstract={The potential of supramolecular solvents (SUPRAS) is investigated for the extraction of bioactive compounds from coffee cherry pulp, one of the major by-products generated in the coffee industry. SUPRAS made up of hexagonal inverted aggregates of octanoic acid in ethanol:water mixtures provided good extraction yields for bioactives (3.6 ± 0.3 mg caffeine g−1 and 0.9 ± 0.1 mg protocatechuic acid g−1) at a low solvent:sample ratio of 4:1 v/w and under mild operations conditions (5 min extraction at room temperature). SUPRAS-based extraction was optimized and extracts were analyzed to identify the main phenolic and alkaloid compounds. A variety of bioactives were present and extracts showed high antioxidant capacity by different assays (45% for DPPH and 91% for ABTS). Extraction efficiencies with SUPRAS were clearly superior than those obtained with organic solvents commonly used for valorization of coffee residues. © 2020 Elsevier Ltd},
author_keywords={Bioactive compounds;  Coffee cherry pulp;  Supramolecular solvents;  Valorization},
keywords={Fatty acids;  Solvent extraction;  Supramolecular chemistry, Antioxidant capacity;  Bioactive compounds;  Coffee cherries;  Extraction efficiencies;  Protocatechuic acid;  Solvent:sample ratios;  Supramolecular solvents;  Valorization, Organic solvents},
